import random
import string

# Function to generate a random string of length n with "rec" prefix
def generate_random_string(n):
    prefix = "rec"
    random_chars = ''.join(random.choice(string.ascii_lowercase + string.digits) for _ in range(n - len(prefix)))
    return prefix + random_chars

# Generate 1000 random strings with "rec" prefix
similar_strings = [generate_random_string(16) for _ in range(1000)]

# Print the strings as an array
print("[")
for string in similar_strings:
    print(f'    "{string}",')
print("]")